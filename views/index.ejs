
<h1><%= title %></h1>
<p>Welcome to
  <%= title %>
</p>
<% if(!user){ %>
  <ul>
    <li><a href="/login">Login</a></li>
    <li><a href="/signup">signup</a></li>
  </ul>
  <% }else{ %>
    <a href="/logout">Logout <%= user.username %></a>
    <% } %>
      <div class="container">
        <form action="">
          <div class="form-group">
            <label for="">¿En qué podemos ayudarte?</label>
            <input type="search" class="form-control">
          </div>
        </form>
      </div>

      <div class="">
        <p>TOP PSICOLOGOS</p>
        <% users.forEach((user) => { %>
          <p>
            <% if(user.role === "psychologist") {%>
            <%= user.name %>
            <a href="/p_profile?id=<%= user._id %>">View Profile</a>
          </p>
        <% }}); %>
      </div>

<div class="">
  <p>TOP CLINICAS DE PSICOLOGIA</p>
  <% users.forEach((user) => { %>
    <p>
      <% if(user.role === "clinic") {%>
      <%= user.name %>
      <a href="/c_profile?id=<%= user._id %>">View Profile</a>
    </p>
  <% }}); %>
</div>
