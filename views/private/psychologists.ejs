<div class="container main-content">
  <h2>Psic√≥logos</h2>
  <ul class="tickets-list">
    <% users.forEach((e) => { %>
      <% if(e.validation === true) { %>
        <li class="tickets-holder">
          <div class="tickets tickets-flex">
            <div>
              <img src="http://comunicaciones.uc.cl/wp-content/uploads/comunicaciones/2014/09/ingrid_bachmann.jpg" alt="">
            </div>
            <div class="tickets-content">
              <h3 class="mb1">
                <%= e.name %>
              </h3>
              <p class="mb1">
                <%= e.address %>
              </p>
              <p class="mb1">
                <%= e.city %>
              </p>
              <a href="/p_profile?id=<%= e._id %>">Pefil</a>
              <% if(user.role === "clinic"){ %>
                <form action="/psychologists" method="post">
                  <input type="hidden" name="id" value="<%= e._id %>">
                  <button class="btn btn-primary">Add</button>
                </form>
                <% } %>
            </div>
          </div>
        </li>
        <% } %>
          <%})%>
  </ul>
</div>
