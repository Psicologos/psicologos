<div class="container main-content clinics">
  <h2>Listado de cl√≠nicas</h2>
  <ul class="tickets-list tickets-list-centered">
    <% users.forEach((e) => { %>
      <% if(e.validation === true) { %>
        <li class="tickets-holder tickets-holder-list">
          <div class="tickets tickets-flex">
            <div>
              <img src="<%= e.photo %>" alt="" class="mr2">
            </div>
            <div class="tickets-content">
              <h3 class="mb1">
              <%= e.name %>
            </h3>
              <p class="mb1">
                <%= e.address %>
              </p>
              <p class="mb1">
                <%= e.city %>
              </p>
              <a href="/c_profile/<%= e._id %>" class="mb1">Pefil</a>
              <form action="/psychologists" method="post" class="text-right">
                <input type="hidden" name="id" value="<%= e._id %>">
              </form>
            </div>
        </li>
        <% } %>
          <%})%>
  </ul>
  </div>
